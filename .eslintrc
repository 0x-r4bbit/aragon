{
  "extends": "airbnb",
  "rules": {
    "semi": ["error", "never"],
    "no-undef": 0,
    "import/no-absolute-path": 0,
    "import/no-extraneous-dependencies": 1,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "arrow-parens": 0,
    "no-new": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "guard-for-in":0
  }
}
