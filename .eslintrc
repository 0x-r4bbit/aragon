{
  "extends": ["airbnb", "airbnb-flow"],
  "rules": {
    "semi": ["error", "never"],
    "no-undef": 0,
    "import/no-absolute-path": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "arrow-parens": 0,
    "no-new": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "func-names": 0,
    "no-alert": 0
  }
}
