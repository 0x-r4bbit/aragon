{
  "name": "aragon",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "MONGO_URL=0 METEOR_OFFLINE_CATALOG=1 meteor",
    "no-metamask-dev": "MONGO_URL=0 METEOR_OFFLINE_CATALOG=1 meteor --settings no-metamask-settings.json",
    "landing": "MONGO_URL=0 METEOR_OFFLINE_CATALOG=1 meteor --settings landing-settings.json",
    "metamask-dev": "cd .metamask; gulp dev --debug=false --aragon=true",
    "testrpc": "testrpc --account='0xa45d10c15047c9d6790de40c8a6871cf71205d9323902738e5da600a786e460e,10000000000000000000000'",
    "app": "./.electron/node_modules/.bin/electron .electron",
    "migrations": "cd imports/lib/contracts; truffle migrate --reset",
    "binaries": "electron-packager .electron/ Aragon --overwrite --platform=darwin --icon=.electron/icon.icns --out .electron/dist"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-runtime": "^6.22.0",
    "chart.js": "^2.4.0",
    "coinr": "^1.1.1",
    "color-scheme": "0.0.5",
    "crypto-js": "^3.1.8",
    "electron": "^1.4.12",
    "ethereumjs-abi": "^0.6.4",
    "ethereumjs-util": "^5.0.1",
    "faker": "^3.1.0",
    "framed-msgpack-rpc": "^1.1.13",
    "intertron-client": "^1.0.2",
    "jdenticon": "^1.4.0",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.17.1",
    "money": "^0.2.0",
    "notifyjs": "^2.0.3",
    "post-message-stream": "^1.0.0",
    "truffle-contract": "^1.1.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "devtron": "^1.4.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.37.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
